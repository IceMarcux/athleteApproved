        <Label Content="OPERATING HOURS: " HorizontalAlignment="Left" Margin="119,326,0,0" VerticalAlignment="Top" Width="126" FontFamily="Rockwell" FontWeight="Bold" FontSize="11" Grid.Column="1"/>
        <Label Content="RENTAL FEE: " HorizontalAlignment="Left" Margin="119,351,0,0" VerticalAlignment="Top" Width="83" FontFamily="Rockwell" FontWeight="Bold" FontSize="11" Grid.Column="1"/>
