        <TextBox HorizontalAlignment="Left" Height="26" Margin="155,666,0,0" TextWrapping="Wrap" Text="ANGAS NG COURT ANG GANDA!" VerticalAlignment="Top" Width="337" FontFamily="Sans Serif Collection" TextChanged="TextBox_TextChanged_1" Grid.Column="1"/>
